{
  "manifest_version": 2,

  "name": "Task Runner",
  "description": "Testing out Chrome extensions",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.3.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "https://www.poshmark.com/"
   ]
}
